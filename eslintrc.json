{
  "rules": {
    "no-alert": 0,                              // alertの使用は許可する
    "no-console": 0,                            // consoleの使用は許可する
    "no-unused-vars": [1, {
      "vars": "all", "args": "after-used"
    }]
  },
  "globals": {
    "Titanium": false,
    "Ti": false,
    "Alloy": false,
    "$": false,
    "_": false,
    "OS_IOS": false,
    "OS_ANDROID": false,
    "OS_MOBILEWEB": false,
    "OS_WINDOWS": false,
    "ENV_DEV": false,
    "ENV_TEST": false,
    "ENV_PRODUCTION": false,
    "DIST_ADHOC": false,
    "DIST_STORE": false,
    "task": false
  }
}
